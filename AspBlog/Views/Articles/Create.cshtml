@model AspBlog.Models.Article

@{
    ViewData["Title"] = "Vytvoř";
}

<h1>Vytvoř</h1>
<h4>Článek</h4>
<hr />

    
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Obsah" class="control-label"></label>
                <input asp-for="Obsah" class="form-control" />
                <span asp-validation-for="Obsah" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Titul" class="control-label"></label>
                <input asp-for="Titul" class="form-control" />
                <span asp-validation-for="Titul" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Popis" class="control-label"></label>
                <input asp-for="Popis" class="form-control" />
                <span asp-validation-for="Popis" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Vytvoř" class="btn btn-primary" />
            </div>
        </form>
  


<div>
    <a asp-action="Index">Zpátky na seznam</a>
</div>

